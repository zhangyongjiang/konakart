            <property name="queryText">SELECT
    ords.*,
    DATE_FORMAT(DATE_SUB(curdate(), INTERVAL id DAY),'%c') month_number,
    DATE_FORMAT(DATE_SUB(curdate(), INTERVAL id DAY),'%b') month_name,
    DATE_FORMAT(DATE_SUB(curdate(), INTERVAL id DAY),'%y') year_number,
    concat(DATE_FORMAT(DATE_SUB(curdate(), INTERVAL id DAY),'%e'), '-',
           DATE_FORMAT(DATE_SUB(curdate(), INTERVAL id DAY),'%b')) ddmon,
    id,
    count(day_number) order_count
    FROM utility left outer join
    (select
            DATE_FORMAT(date_purchased,'%e') day_number
      from orders
      where DATE_SUB(curdate(), INTERVAL 31 DAY) &lt; date_purchased
      and DATE_SUB(curdate(), INTERVAL 1 DAY) > date_purchased) ords
    on utility.id = day_number
where id > 0 and id &lt; 31
group by id
order by id desc</property>
