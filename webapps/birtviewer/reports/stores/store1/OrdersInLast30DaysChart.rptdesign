            <method name="beforeOpen"><![CDATA[importPackage(Packages.java.util.logging);
importPackage(Packages.com.konakart.reports);

//var kkLog = Logger.getLogger("com.konakart.reports");
//kkLog.fine("OrdersInLast30Days.beforeOpen() Set the appropriate query for the dbType"); 

var storeId = params["storeId"];
dbparams = reportContext.getGlobalVariable("dbParameters");

dbType = dbparams.getDbType();

//kkLog.fine("OrdersInLast30Days.beforeOpen() DataSource dbType = " + dbType);

engineMode = dbparams.getEngineMode();

//kkLog.fine("OrdersInLast30Days.beforeOpen() Engine Mode = " + engineMode);

storeWhereAnd = "";

if (engineMode == 2)
{
	storeWhereAnd = " orders.store_id = '" + storeId + "' and ";
}

if (dbType == "postgresql")
{
this.queryText = "SELECT  ords.*, to_char(current_timestamp-(id*INTERVAL '1 DAY'),'MM') as month_number, to_char(current_timestamp-(id*INTERVAL '1 DAY'),'Mon') as month_name,    to_char(current_timestamp-(id*INTERVAL '1 DAY'),'YY') as year_number, to_char(current_timestamp-(id*INTERVAL '1 DAY'),'DD') || '-' || to_char(current_timestamp-(id*INTERVAL '1 DAY'),'Mon') as ddmon, id, count(day_number) as order_count    FROM utility left outer join (select EXTRACT(DAY from date_purchased)as day_number from orders where " + storeWhereAnd + " (current_timestamp - INTERVAL '31 DAY' < date_purchased) and (current_timestamp - INTERVAL '1 DAY' > date_purchased)) as ords on date_part('day', current_timestamp-(utility.id*INTERVAL '1 DAY')) = ords.day_number where id >0 and id < 31  group by id, ords.day_number order by id desc";
} else if (dbType == "oracle")
{
this.queryText = "SELECT  ords.*, to_char(current_timestamp-(id*INTERVAL '1' DAY),'MM') month_number, to_char(current_timestamp-(id*INTERVAL '1' DAY),'Mon') month_name,    to_char(current_timestamp-(id*INTERVAL '1' DAY),'YY') year_number, to_char(current_timestamp-(id*INTERVAL '1' DAY),'DD') || '-' || to_char(current_timestamp-(id*INTERVAL '1' DAY),'Mon') ddmon, id, count(day_number) order_count    FROM utility left outer join (select EXTRACT(DAY from date_purchased) day_number from orders where " + storeWhereAnd + " (current_timestamp - INTERVAL '31' DAY < date_purchased) and (current_timestamp - INTERVAL '1' DAY > date_purchased)) ords on to_char(current_timestamp-(id*INTERVAL '1' DAY),'DD') = day_number where id >0 and id < 31  group by id, ords.day_number order by id desc";
} else if (dbType == "db2net")
{
this.queryText = "SELECT  ords.*, char(month(current_timestamp - id DAYS)) month_number, substr(monthname(current timestamp - id DAYS),1,3) month_name, substr(char(year(current timestamp - id DAYS)),3,2) year_number, rtrim(char(day(current timestamp - id DAYS))) || '-' || substr(monthname(current timestamp - id DAYS),1,3) ddmon, id, count(day_number) order_count FROM utility left outer join (select DAY(date_purchased) day_number from orders where " + storeWhereAnd + " (current timestamp - 31 DAYS < date_purchased) and (current timestamp - 1 DAYS > date_purchased)) ords on DAY(current timestamp - id DAYS) = day_number where id > 0 and id < 31 group by id, ords.day_number order by id desc";
} else if (dbType == "mssql")
{
this.queryText = "SELECT 1 day_number, MONTH(DATEADD(day, -id, current_timestamp)) month_number, CAST(DATEADD(day, -id, current_timestamp) as char(3)) month_name, YEAR(DATEADD(day, -id, current_timestamp)) year_number, CAST(DAY(DATEADD(day, -id, current_timestamp)) as nvarchar(2)) + '-' + CAST(DATEADD(day, -id, current_timestamp) as char(3)) ddmon, id, count(day_number) order_count FROM utility left outer join (select DAY(date_purchased) day_number from orders where " + storeWhereAnd + " DATEADD(day, -31, current_timestamp) < date_purchased and DATEADD(day, -1, current_timestamp) > date_purchased) ords on DAY(DATEADD(day, -id, current_timestamp)) = day_number where id > 0 and id < 31 group by id order by id desc";
} 
else
{
this.queryText = "SELECT  ords.*, DATE_FORMAT(DATE_SUB(curdate(), INTERVAL id DAY),'%c') month_number, DATE_FORMAT(DATE_SUB(curdate(), INTERVAL id DAY),'%b') month_name,     DATE_FORMAT(DATE_SUB(curdate(), INTERVAL id DAY),'%y') year_number, concat(DATE_FORMAT(DATE_SUB(curdate(), INTERVAL id DAY),'%e'), '-', DATE_FORMAT(DATE_SUB(curdate(), INTERVAL id DAY),'%b')) ddmon, id, count(day_number) order_count FROM utility left outer join (select DATE_FORMAT(date_purchased,'%e') day_number from orders where " + storeWhereAnd + " DATE_SUB(curdate(), INTERVAL 31 DAY) < date_purchased and DATE_SUB(curdate(), INTERVAL 1 DAY) > date_purchased) ords on DATE_FORMAT(DATE_SUB(curdate(), INTERVAL id DAY),'%e') = day_number where id >0 and id < 31 group by id order by id desc";
}

//kkLog.info("OrdersInLast30Days.beforeOpen() SQL = " + this.queryText);]]></method>
